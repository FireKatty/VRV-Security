{
    "version": 2,
    "name": "vrv-security",
    "builds": [
      {
        "src": "backend-server.js",
        "use": "@vercel/node"
      },
      {
        "src": "rbac-ui/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "rbac-ui/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend-server.js"
      },
      {
        "src": "/(.*)",
        "dest": "rbac-ui/$1"
      }
    ]
}
  